    "permissions": [{
        "resource": "users",
        "methods": [
            "POST"
        ],
        "action": "deny"
    }, {
        "resource": "analyzers",
        "methods": [
            "GET"
        ],
        "action": "allow"
    }, {
        "resource": "analyzer",
        "methods": [
            "GET"
        ],
